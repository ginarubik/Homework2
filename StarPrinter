import java.util.Scanner;

public class StarPrinter {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Adj meg egy sz√°mot (1-20): ");
        int num = scanner.nextInt();

        for (int i = 0; i < num; i++) {
            System.out.print("*");
        }
    }
}
